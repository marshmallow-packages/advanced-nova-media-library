{
    "name": "marshmallow/advanced-nova-media-library",
    "description": "Laravel Nova tools for managing the Spatie media library.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "laravel/nova": "^5.0",
        "spatie/laravel-medialibrary": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\AdvancedNovaMediaLibrary\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\AdvancedNovaMediaLibrary\\AdvancedNovaMediaLibraryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
